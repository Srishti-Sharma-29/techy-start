#include <stdio.h>

int main() {

  int a, b, c;

  printf("Enter three different numbers: ");
  scanf("%d %d %d", &a, &b, &c);

  // if a is greater than b and c, a is the largest
  if (a >= b && a >= c)
    printf("The largest number is %d", a);

  // if b is greater than a and c, b is the largest
  if (b >= a && b >= c)
    printf("The largest number is %d", b);

  // if c is greater than a and b, c is the largest
  if (c >= a && c >= b)
    printf("The largest number is %d", c);
  return 0;
}
